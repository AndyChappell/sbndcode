  ReferenceLabSolidAngle: @local::sbnd_bnb_solid_angle_box
  ReferenceKaonEnergy: @local::kaon_energy
}

weighted_ray_trace_box: {
  tool_type: WeightedRayTraceBox
  Box: @local::sbnd_detector_box
  ReferenceLabSolidAngle: @local::sbnd_bnb_solid_angle_box
